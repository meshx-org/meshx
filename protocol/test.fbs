namespace users;

attribute "priority";

enum Color : byte { Red = 1, Green, Blue }

union Request { ManyHellosRequest, HelloRequest }

table User {
  name: string;
  id: ulong;
  flex: [ubyte] (flexbuffer);
}

table HelloReply {
  message:string;
}

table HelloRequest {
  name:string;
}

table ManyHellosRequest {
  name:string;
  num_greetings:int;
}

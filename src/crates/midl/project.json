{
    "name": "midl",
    "root": "./src/crates/midl",
    "projectType": "application",
    "implicitDependencies": ["fiber_kernel"],
    "targets": {
        "build": {
            "executor": "@nxrs/cargo:build",
            "options": {
                "release": false
            },
            "configurations": {
                "production": {
                    "release": true
                }
            }
        },
        "run": {
            "executor": "@nxrs/cargo:build",
            "options": {
                "release": false,
                "run": true
            }
        },
        "test": {
            "executor": "@nxrs/cargo:test",
            "options": {}
        },
        "lint": {
            "executor": "@nxrs/cargo:clippy",
            "options": {
                "fix": false,
                "failOnWarnings": true,
                "noDeps": true
            }
        }
    },
    "tags": []
}

{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "required": [
    "srcs"
  ],
  "properties": {
    "midlJson": {
      "type": "string"
    },
    "srcs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "language": {
      "type": "string",
      "enum": [
        "ts",
        "rust"
      ]
    },
    "outDir": {
      "type": "string"
    }
  }
}
